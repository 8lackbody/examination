<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context=".activity.AllActivity"
    tools:showIn="@layout/activity_all"
    android:background="#F0F0F0">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="100dp"
        android:background="@drawable/linearlayout_top"
        tools:ignore="MissingConstraints">

    </LinearLayout>

    <LinearLayout
        android:layout_width="350dp"
        android:layout_height="500dp"
        android:background="@drawable/linearlayout_boder"
        android:orientation="vertical"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="30dp"
        tools:layout_editor_absoluteY="135dp">

        <LinearLayout
            android:layout_width="300dp"
            android:layout_height="400dp"
            android:layout_gravity="center"
            android:layout_marginTop="25dp"
            android:background="@drawable/white_roundframe"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="280dp"
                android:layout_height="100dp"
                android:layout_gravity="center"
                android:layout_marginTop="10dp">

                <LinearLayout
                    android:layout_width="200dp"
                    android:layout_height="match_parent"
                    android:orientation="vertical">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="50dp">

                        <TextView
                            android:layout_width="90dp"
                            android:layout_height="30dp"
                            android:layout_gravity="center"
                            android:layout_marginRight="10dp"
                            android:text="开始标签号"
                            android:textColor="@color/black"
                            android:textSize="20dp"
                            android:textStyle="bold"
                            android:gravity="center">
                        </TextView>

                        <EditText
                            android:layout_width="100dp"
                            android:layout_height="30dp"
                            android:layout_gravity="center"
                            android:background="@drawable/set_edittext_backgroud">
                        </EditText>

                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="50dp">

                        <TextView
                            android:layout_width="90dp"
                            android:layout_height="30dp"
                            android:layout_gravity="center"
                            android:layout_marginRight="10dp"
                            android:text="结束标签号"
                            android:textColor="@color/black"
                            android:textSize="20dp"
                            android:textStyle="bold"
                            android:gravity="center">
                        </TextView>

                        <EditText
                            android:layout_width="100dp"
                            android:layout_height="30dp"
                            android:layout_gravity="center"
                            android:background="@drawable/set_edittext_backgroud">
                        </EditText>

                    </LinearLayout>

                </LinearLayout>

                <LinearLayout
                    android:layout_width="80dp"
                    android:layout_height="match_parent"
                    android:orientation="vertical">
                    
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="40dp"
                        android:text="已查找数"
                        android:textColor="@color/black"
                        android:textSize="18dp"
                        android:textStyle="bold"
                        android:gravity="center">

                    </TextView>

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="60dp"
                        android:text="88"
                        android:textColor="@color/black"
                        android:textSize="20dp"
                        android:textStyle="bold"
                        android:gravity="center">

                    </TextView>

                </LinearLayout>

            </LinearLayout>

            <LinearLayout
                android:layout_width="280dp"
                android:layout_height="270dp"
                android:layout_gravity="center"
                android:layout_marginTop="10dp">

                <LinearLayout
                    android:layout_width="80dp"
                    android:layout_height="match_parent"
                    android:orientation="vertical">

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="40dp"
                        android:text="序号"
                        android:textColor="@color/black"
                        android:textSize="18dp"
                        android:textStyle="bold"
                        android:gravity="center"
                        android:background="@drawable/black_frame">

                    </TextView>

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="230dp"
                        android:text="1"
                        android:textColor="@color/black"
                        android:textSize="18dp"
                        android:textStyle="bold"
                        android:background="@drawable/black_frame">

                    </TextView>

                </LinearLayout>

                <LinearLayout
                    android:layout_width="200dp"
                    android:layout_height="match_parent"
                    android:orientation="vertical">

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="40dp"
                        android:text="EPC"
                        android:textColor="@color/black"
                        android:textSize="18dp"
                        android:textStyle="bold"
                        android:gravity="center"
                        android:background="@drawable/black_frame">

                    </TextView>

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="230dp"
                        android:text="111111111111"
                        android:textColor="@color/black"
                        android:textSize="18dp"
                        android:textStyle="bold"
                        android:background="@drawable/black_frame">

                    </TextView>

                </LinearLayout>

            </LinearLayout>

        </LinearLayout>

        <LinearLayout
            android:layout_width="300dp"
            android:layout_height="50dp"
            android:layout_gravity="center"
            android:layout_marginTop="10dp">

            <Button
                android:layout_width="80dp"
                android:layout_height="match_parent"
                android:background="@drawable/white_roundframe"
                android:text="SUBMIT"
                android:textColor="@color/black"
                android:textSize="20dp"
                android:textStyle="bold">

            </Button>

            <Button
                android:layout_width="80dp"
                android:layout_height="match_parent"
                android:background="@drawable/white_roundframe"
                android:layout_marginLeft="30dp"
                android:text="START"
                android:textColor="@color/black"
                android:textSize="20dp"
                android:textStyle="bold">

            </Button>

            <Button
                android:layout_width="80dp"
                android:layout_height="match_parent"
                android:background="@drawable/white_roundframe"
                android:layout_marginLeft="30dp"
                android:text="EMPITY"
                android:textColor="@color/black"
                android:textSize="20dp"
                android:textStyle="bold">

            </Button>

        </LinearLayout>

    </LinearLayout>


</androidx.constraintlayout.widget.ConstraintLayout>